<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="CARET_BLINKING_PERIOD" value="350" />
  </component>
  <component name="LiveEditOptions">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="SqlEditorOptions">
    <option name="aliasQualification" value="Never" />
    <option name="namespaceQualification" value="Never" />
    <option name="tableQualification" value="Never" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\btodo-later\b.*" />
  </component>
</application>